{
  "extends": ["eslint-config-prettier", "react-app", "plugin:react-hooks/recommended"],
  "rules": {
    "newline-before-return": "error",
    "no-console": "error",
    "@typescript-eslint/no-unused-vars": "error",
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "default-case": "error",
    "no-nested-ternary": "error",
    "max-len": [
      "error",
      {
        "code": 120,
        "tabWidth": 4
      }
    ],
    "max-lines": ["error", 600],
    "import/order": [
      "error",
      {
        "newlines-between": "always"
      }
    ]
  }
}
